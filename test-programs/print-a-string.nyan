loop: LDX char_one // at the beginning of the program, IDX is set to 0, so loads char_one
OUT
INC IDX // increment IDX

// if idx != str_len then jmp back to loop
CMX str_len
JNE loop

LDM #10
OUT

END

// our own implementation of a string
current_index: DAT #0
str_len: DAT #3
char_one: DAT #65 // A
char_two: DAT #68 // D
char_three: DAT #73 // I